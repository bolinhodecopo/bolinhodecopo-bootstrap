.app-nav
  background #efecdb
  margin 50px auto 0 auto
  font-family 'Homemade Apple', cursive
  position relative
  z-index 1

  ul
    margin 0
    padding 20px 0
    border-top 3px dashed #8a6044
    border-bottom 3px dashed #8a6044

  .links-list
    margin 0 auto
    text-align center

    li
      margin 0 20px
      display inline

      a
        font-size 1.3em
